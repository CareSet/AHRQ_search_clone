                            <li><a href="/web/20180620204830/https://www.qualitymeasures.ahrq.gov/summaries/citedownload/28318?format=ris">Reference Manager</a></li>
                        </ul>
                    </div>
                    <!-- /.dropdown-panel -->
                </li>
                                
                <li class="tool">
                        <a href="#" id="28318" data-alert="SignInLanding" class="tool-button save-link">Save</a>
                </li>
            </ul>

            <!-- Accordion -->
