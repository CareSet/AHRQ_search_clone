 (63995005), <a href="/web/20180712190235/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0455099&amp;fLockTerm=Cleaning+wound">Cleaning wound</a>
 (225150002), <a href="/web/20180712190235/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0436149&amp;fLockTerm=Examination+of+skin">Examination of skin</a>
 (271303006), <a href="/web/20180712190235/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0013119&amp;fLockTerm=Medical+dressing">Medical dressing</a>
 (333453004), <a href="/web/20180712190235/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C1258068&amp;fLockTerm=Nutrition+management">Nutrition management</a>
 (386372009), <a href="/web/20180712190235/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C1561964&amp;fLockTerm=Positioning+patient">Positioning patient</a>
 (229824005), <a href="/web/20180712190235/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0011127&amp;fLockTerm=Pressure+ulcer">Pressure ulcer</a>
 (399912005), <a href="/web/20180712190235/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0011127&amp;fLockTerm=Pressure+ulcer">Pressure ulcer</a>
 (420226006), <a href="/web/20180712190235/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0558031&amp;fLockTerm=Pressure+ulcer+assessment">Pressure ulcer assessment</a>
 (225393005), <a href="/web/20180712190235/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0558009&amp;fLockTerm=Pressure+ulcer+care">Pressure ulcer care</a>
 (225357008), <a href="/web/20180712190235/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0150315&amp;fLockTerm=Pressure+ulcer+prevention">Pressure ulcer prevention</a>
 (225996006), <a href="/web/20180712190235/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C1279756&amp;fLockTerm=Procedures+to+aid+continence">Procedures to aid continence</a>
 (225427003), <a href="/web/20180712190235/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0086930&amp;fLockTerm=Risk+assessment">Risk assessment</a>
 (225338004), <a href="/web/20180712190235/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0035648&amp;fLockTerm=Risk+factor">Risk factor</a>
 (80943009), <a href="/web/20180712190235/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0011625&amp;fLockTerm=Skin+agent">Skin agent</a>
 (65628000), <a href="/web/20180712190235/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0150773&amp;fLockTerm=Skin+care">Skin care</a>
 (225360001), <a href="/web/20180712190235/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0013787&amp;fLockTerm=Therapeutic+electrical+stimulation">Therapeutic electrical stimulation</a>
 (169430000), <a href="/web/20180712190235/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0013787&amp;fLockTerm=Therapeutic+electrical+stimulation">Therapeutic electrical stimulation</a>
 (57942008), <a href="/web/20180712190235/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0043240&amp;fLockTerm=Tissue+repair">Tissue repair</a>
 (18867006), <a href="/web/20180712190235/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0043240&amp;fLockTerm=Tissue+repair">Tissue repair</a>
 (298005009), <a href="/web/20180712190235/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0886052&amp;fLockTerm=Wound+care">Wound care</a>
 (225358003)                    </div>
                    <!-- /.field-content -->
                </div>
                <!-- /.field -->
                <div class="field">
                    <h5 class="field-label">SPN</h5>
                    <div class="field-content">
<a href="/web/20180712190235/https://www.guideline.gov/search?f_SPN_CUI=C0013119&amp;fLockTerm=DRESSING">DRESSING</a>
                    </div>
                    <!-- /.field-content -->
                </div>
