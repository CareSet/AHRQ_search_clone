 (234087005), <a href="/web/20180713033428/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0024228&amp;fLockTerm=Disorder+of+lymphatic+system">Disorder of lymphatic system</a>
 (362971004), <a href="/web/20180713033428/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C1285573&amp;fLockTerm=Genotype+determination">Genotype determination</a>
 (363779003), <a href="/web/20180713033428/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C1285573&amp;fLockTerm=Genotype+determination">Genotype determination</a>
 (726528006), <a href="/web/20180713033428/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0021390&amp;fLockTerm=Inflammatory+bowel+disease">Inflammatory bowel disease</a>
 (24526004), <a href="/web/20180713033428/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0598798&amp;fLockTerm=Lymphoid+neoplasm">Lymphoid neoplasm</a>
 (414628006), <a href="/web/20180713033428/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0000618&amp;fLockTerm=Mercaptopurine">Mercaptopurine</a>
 (1039008), <a href="/web/20180713033428/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0000618&amp;fLockTerm=Mercaptopurine">Mercaptopurine</a>
 (386835005), <a href="/web/20180713033428/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0023470&amp;fLockTerm=Myeloid+leukemia">Myeloid leukemia</a>
 (128934006), <a href="/web/20180713033428/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0023470&amp;fLockTerm=Myeloid+leukemia">Myeloid leukemia</a>
 (188732008), <a href="/web/20180713033428/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0023470&amp;fLockTerm=Myeloid+leukemia">Myeloid leukemia</a>
 (37810007), <a href="/web/20180713033428/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C1138555&amp;fLockTerm=Pharmacogenomic+specialist">Pharmacogenomic specialist</a>
 (720505003), <a href="/web/20180713033428/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0003873&amp;fLockTerm=Rheumatoid+arthritis">Rheumatoid arthritis</a>
 (69896004), <a href="/web/20180713033428/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0039902&amp;fLockTerm=Thioguanine">Thioguanine</a>
 (387407006), <a href="/web/20180713033428/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0039902&amp;fLockTerm=Thioguanine">Thioguanine</a>
 (72623000)                    </div>
                    <!-- /.field-content -->
                </div>
