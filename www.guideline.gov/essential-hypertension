 (41549009), <a href="/web/20180712202613/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0001645&amp;fLockTerm=beta-Blocking+agent">beta-Blocking agent</a>
 (33252009), <a href="/web/20180712202613/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0001645&amp;fLockTerm=beta-Blocking+agent">beta-Blocking agent</a>
 (373254001), <a href="/web/20180712202613/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0026426&amp;fLockTerm=Blood+pressure+monitoring">Blood pressure monitoring</a>
 (135840009), <a href="/web/20180712202613/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0005824&amp;fLockTerm=Blood+pressure+taking">Blood pressure taking</a>
 (46973005), <a href="/web/20180712202613/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0006684&amp;fLockTerm=Calcium+channel+blocker">Calcium channel blocker</a>
 (373304005), <a href="/web/20180712202613/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0006684&amp;fLockTerm=Calcium+channel+blocker">Calcium channel blocker</a>
 (48698004), <a href="/web/20180712202613/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0740213&amp;fLockTerm=Diabetes+mellitus+screening">Diabetes mellitus screening</a>
 (171183004), <a href="/web/20180712202613/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0012798&amp;fLockTerm=Diuretic">Diuretic</a>
 (30492008), <a href="/web/20180712202613/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0012798&amp;fLockTerm=Diuretic">Diuretic</a>
 (372695000), <a href="/web/20180712202613/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0085580&amp;fLockTerm=Essential+hypertension">Essential hypertension</a>
 (59621000), <a href="/web/20180712202613/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0020538&amp;fLockTerm=Hypertensive+disorder">Hypertensive disorder</a>
 (38341003), <a href="/web/20180712202613/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0003364&amp;fLockTerm=Hypotensive+agent">Hypotensive agent</a>
 (1182007), <a href="/web/20180712202613/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0003364&amp;fLockTerm=Hypotensive+agent">Hypotensive agent</a>
 (372586001), <a href="/web/20180712202613/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0023676&amp;fLockTerm=Life+style">Life style</a>
 (134436002), <a href="/web/20180712202613/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0023676&amp;fLockTerm=Life+style">Life style</a>
 (60134006), <a href="/web/20180712202613/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0030688&amp;fLockTerm=Patient+education">Patient education</a>
 (311401005), <a href="/web/20180712202613/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0012802&amp;fLockTerm=Thiazide+diuretic">Thiazide diuretic</a>
 (372747003), <a href="/web/20180712202613/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0012802&amp;fLockTerm=Thiazide+diuretic">Thiazide diuretic</a>
 (45518007)                    </div>
                    <!-- /.field-content -->
                </div>
                <!-- /.field -->
    </div>
