 (35321007), <a href="/web/20180712195202/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0024485&amp;fLockTerm=Magnetic+resonance+imaging">Magnetic resonance imaging</a>
 (113091000), <a href="/web/20180712195202/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0024485&amp;fLockTerm=Magnetic+resonance+imaging">Magnetic resonance imaging</a>
 (312250003), <a href="/web/20180712195202/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0336660&amp;fLockTerm=Magnetic+resonance+imaging+unit">Magnetic resonance imaging unit</a>
 (90003000), <a href="/web/20180712195202/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0006264&amp;fLockTerm=Neoplasm+of+bronchus">Neoplasm of bronchus</a>
 (126705004), <a href="/web/20180712195202/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0024121&amp;fLockTerm=Neoplasm+of+lung">Neoplasm of lung</a>
 (126713003), <a href="/web/20180712195202/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0007131&amp;fLockTerm=Non-small+cell+lung+cancer">Non-small cell lung cancer</a>
 (254637007), <a href="/web/20180712195202/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0034606&amp;fLockTerm=Nuclear+medicine+imaging+procedure">Nuclear medicine imaging procedure</a>
 (373205008), <a href="/web/20180712195202/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0039985&amp;fLockTerm=Plain+chest+X-ray">Plain chest X-ray</a>
 (399208008), <a href="/web/20180712195202/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0032743&amp;fLockTerm=Positron+emission+tomography">Positron emission tomography</a>
 (363678002), <a href="/web/20180712195202/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0032743&amp;fLockTerm=Positron+emission+tomography">Positron emission tomography</a>
 (82918005), <a href="/web/20180712195202/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0182349&amp;fLockTerm=Positron+emission+tomography+unit">Positron emission tomography unit</a>
 (39821008), <a href="/web/20180712195202/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0182349&amp;fLockTerm=Positron+emission+tomography+unit">Positron emission tomography unit</a>
 (462323006), <a href="/web/20180712195202/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0149925&amp;fLockTerm=Small+cell+carcinoma+of+lung">Small cell carcinoma of lung</a>
 (254632001), <a href="/web/20180712195202/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0149925&amp;fLockTerm=Small+cell+carcinoma+of+lung">Small cell carcinoma of lung</a>
