 (1759002), <a href="/web/20180712202830/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0028708&amp;fLockTerm=Assessment+of+nutritional+status">Assessment of nutritional status</a>
 (310243009), <a href="/web/20180712202830/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0005890&amp;fLockTerm=Body+height+measure">Body height measure</a>
 (50373000), <a href="/web/20180712202830/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C1305855&amp;fLockTerm=Body+mass+index">Body mass index</a>
 (60621009), <a href="/web/20180712202830/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0005910&amp;fLockTerm=Body+weight">Body weight</a>
 (27113001), <a href="/web/20180712202830/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0012173&amp;fLockTerm=Dietary+fiber">Dietary fiber</a>
 (116272000), <a href="/web/20180712202830/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0418859&amp;fLockTerm=Education+about+alcohol+consumption">Education about alcohol consumption</a>
 (281078001), <a href="/web/20180712202830/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0085207&amp;fLockTerm=Gestational+diabetes+mellitus">Gestational diabetes mellitus</a>
 (11687002), <a href="/web/20180712202830/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0026162&amp;fLockTerm=Mineral">Mineral</a>
 (87918000), <a href="/web/20180712202830/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0006777&amp;fLockTerm=Nutrient+intake">Nutrient intake</a>
 (162533000), <a href="/web/20180712202830/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0006777&amp;fLockTerm=Nutrient+intake">Nutrient intake</a>
 (226320007), <a href="/web/20180712202830/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C1258068&amp;fLockTerm=Nutrition+management">Nutrition management</a>
 (386372009), <a href="/web/20180712202830/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C2937349&amp;fLockTerm=Nutrition+therapy">Nutrition therapy</a>
 (386373004), <a href="/web/20180712202830/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0392209&amp;fLockTerm=Nutritional+status">Nutritional status</a>
 (87276001), <a href="/web/20180712202830/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0420347&amp;fLockTerm=Patient+referral+to+dietitian">Patient referral to dietitian</a>
 (103699006), <a href="/web/20180712202830/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0026606&amp;fLockTerm=Physical+activity">Physical activity</a>
 (48761009), <a href="/web/20180712202830/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0026606&amp;fLockTerm=Physical+activity">Physical activity</a>
 (68130003), <a href="/web/20180712202830/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0453863&amp;fLockTerm=Snack+food">Snack food</a>
 (228070005), <a href="/web/20180712202830/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0042890&amp;fLockTerm=Vitamin">Vitamin</a>
 (12968008), <a href="/web/20180712202830/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0042890&amp;fLockTerm=Vitamin">Vitamin</a>
 (87708000)                    </div>
                    <!-- /.field-content -->
                </div>
                <!-- /.field -->
    </div>
        <!-- /.content-tab-panel -->
            <div id="neats-tab" class="content-tab-panel">
