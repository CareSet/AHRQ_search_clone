 (363505006), <a href="/web/20180609184056/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0026640&amp;fLockTerm=Neoplasm+of+mouth">Neoplasm of mouth</a>
 (126797001), <a href="/web/20180609184056/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0026640&amp;fLockTerm=Neoplasm+of+mouth">Neoplasm of mouth</a>
 (235075007), <a href="/web/20180609184056/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0037579&amp;fLockTerm=Neoplasm+of+soft+tissue">Neoplasm of soft tissue</a>
 (387837005), <a href="/web/20180609184056/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0376306&amp;fLockTerm=Oral+examination">Oral examination</a>
 (122856003), <a href="/web/20180609184056/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0031809&amp;fLockTerm=Physical+examination">Physical examination</a>
 (302199004), <a href="/web/20180609184056/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0031809&amp;fLockTerm=Physical+examination">Physical examination</a>
 (5880005), <a href="/web/20180609184056/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0031809&amp;fLockTerm=Physical+examination">Physical examination</a>
 (81375008), <a href="/web/20180609184056/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C1299295&amp;fLockTerm=Primary+malignant+neoplasm+of+oral+cavity">Primary malignant neoplasm of oral cavity</a>
 (372001002), <a href="/web/20180609184056/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0199230&amp;fLockTerm=Screening+for+cancer">Screening for cancer</a>
 (15886004), <a href="/web/20180609184056/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0424945&amp;fLockTerm=Social+%2f+personal+history+observable">Social / personal history observable</a>
 (160476009), <a href="/web/20180609184056/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0007137&amp;fLockTerm=Squamous+cell+carcinoma">Squamous cell carcinoma</a>
 (28899001), <a href="/web/20180609184056/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0007137&amp;fLockTerm=Squamous+cell+carcinoma">Squamous cell carcinoma</a>
 (402815007), <a href="/web/20180609184056/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0585362&amp;fLockTerm=Squamous+cell+carcinoma+of+mouth">Squamous cell carcinoma of mouth</a>
 (307502000), <a href="/web/20180609184056/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0280313&amp;fLockTerm=Squamous+cell+carcinoma+of+oropharynx">Squamous cell carcinoma of oropharynx</a>
 (423464009), <a href="/web/20180609184056/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0280313&amp;fLockTerm=Squamous+cell+carcinoma+of+oropharynx">Squamous cell carcinoma of oropharynx</a>
 (707585008)                    </div>
                    <!-- /.field-content -->
                </div>
                <!-- /.field -->
    </div>
        <!-- /.content-tab-panel -->
