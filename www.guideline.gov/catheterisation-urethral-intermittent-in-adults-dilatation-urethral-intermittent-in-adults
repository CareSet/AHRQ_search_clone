 (30253003), <a href="/web/20180712190252/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0860239&amp;fLockTerm=Infection+associated+with+catheter">Infection associated with catheter</a>
 (440653007), <a href="/web/20180712190252/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0150397&amp;fLockTerm=Intermittent+urethral+catheterization">Intermittent urethral catheterization</a>
 (225103006), <a href="/web/20180712190252/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0282222&amp;fLockTerm=Lubricant">Lubricant</a>
 (312438007), <a href="/web/20180712190252/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0282222&amp;fLockTerm=Lubricant">Lubricant</a>
 (350100009), <a href="/web/20180712190252/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0282222&amp;fLockTerm=Lubricant">Lubricant</a>
 (706774004), <a href="/web/20180712190252/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C3251814&amp;fLockTerm=Measurement+of+fluid+intake">Measurement of fluid intake</a>
 (711006002), <a href="/web/20180712190252/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0030688&amp;fLockTerm=Patient+education">Patient education</a>
 (311401005), <a href="/web/20180712190252/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0455082&amp;fLockTerm=Skin+cleansing+procedure">Skin cleansing procedure</a>
 (225133004), <a href="/web/20180712190252/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0041618&amp;fLockTerm=Ultrasonography">Ultrasonography</a>
 (16310003), <a href="/web/20180712190252/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0041618&amp;fLockTerm=Ultrasonography">Ultrasonography</a>
 (278292003), <a href="/web/20180712190252/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0179800&amp;fLockTerm=Urethral+catheter">Urethral catheter</a>
 (34759008), <a href="/web/20180712190252/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C1455681&amp;fLockTerm=Urethral+catheterization">Urethral catheterization</a>
 (410021007), <a href="/web/20180712190252/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0878948&amp;fLockTerm=Urethral+dilatation+catheter">Urethral dilatation catheter</a>
 (464900000), <a href="/web/20180712190252/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0203852&amp;fLockTerm=Urinary+bladder+residual+urine+study">Urinary bladder residual urine study</a>
 (6480006), <a href="/web/20180712190252/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0179802&amp;fLockTerm=Urinary+catheter">Urinary catheter</a>
 (20568009), <a href="/web/20180712190252/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C1444394&amp;fLockTerm=Urinary+catheter+insertion+education">Urinary catheter insertion education</a>
 (410255002), <a href="/web/20180712190252/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C1444395&amp;fLockTerm=Urinary+catheter+insertion+management">Urinary catheter insertion management</a>
 (410256001), <a href="/web/20180712190252/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0444303&amp;fLockTerm=Urinary+catheter+tip">Urinary catheter tip</a>
 (258641000), <a href="/web/20180712190252/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0430970&amp;fLockTerm=Urinary+frequency+volume+chart">Urinary frequency volume chart</a>
 (252908007), <a href="/web/20180712190252/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C1287298&amp;fLockTerm=Urine+volume+finding">Urine volume finding</a>
 (251975009), <a href="/web/20180712190252/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C1287298&amp;fLockTerm=Urine+volume+finding">Urine volume finding</a>
 (364202003), <a href="/web/20180712190252/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C1287298&amp;fLockTerm=Urine+volume+finding">Urine volume finding</a>
 (365678000), <a href="/web/20180712190252/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C1287298&amp;fLockTerm=Urine+volume+finding">Urine volume finding</a>
 (366282004)                    </div>
                    <!-- /.field-content -->
                </div>
                <!-- /.field -->
