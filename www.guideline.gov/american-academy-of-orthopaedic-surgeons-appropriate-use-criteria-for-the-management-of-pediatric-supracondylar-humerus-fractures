 (77444004), <a href="/web/20180712194909/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0187016&amp;fLockTerm=Closed+reduction+of+fracture+of+humerus">Closed reduction of fracture of humerus</a>
 (39706009), <a href="/web/20180712194909/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0435569&amp;fLockTerm=Closed+supracondylar+fracture+of+humerus">Closed supracondylar fracture of humerus</a>
 (58580000), <a href="/web/20180712194909/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0016641&amp;fLockTerm=Fixation+of+fracture">Fixation of fracture</a>
 (239279000), <a href="/web/20180712194909/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0407250&amp;fLockTerm=Fixation+of+fracture+using+pins">Fixation of fracture using pins</a>
 (239280002), <a href="/web/20180712194909/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0016658&amp;fLockTerm=Fracture">Fracture</a>
 (125605004), <a href="/web/20180712194909/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0016658&amp;fLockTerm=Fracture">Fracture</a>
 (72704001), <a href="/web/20180712194909/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0020162&amp;fLockTerm=Fracture+of+humerus">Fracture of humerus</a>
 (66308002), <a href="/web/20180712194909/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0024485&amp;fLockTerm=Magnetic+resonance+imaging">Magnetic resonance imaging</a>
 (113091000), <a href="/web/20180712194909/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0024485&amp;fLockTerm=Magnetic+resonance+imaging">Magnetic resonance imaging</a>
 (312250003), <a href="/web/20180712194909/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0336660&amp;fLockTerm=Magnetic+resonance+imaging+unit">Magnetic resonance imaging unit</a>
 (90003000), <a href="/web/20180712194909/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0027853&amp;fLockTerm=Neurological+assessment">Neurological assessment</a>
 (225398001), <a href="/web/20180712194909/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0027853&amp;fLockTerm=Neurological+assessment">Neurological assessment</a>
 (84728005), <a href="/web/20180712194909/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0186772&amp;fLockTerm=Open+reduction+of+fracture+of+humerus">Open reduction of fracture of humerus</a>
 (85813009), <a href="/web/20180712194909/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0435570&amp;fLockTerm=Open+supracondylar+fracture+of+humerus">Open supracondylar fracture of humerus</a>
 (62356006), <a href="/web/20180712194909/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0030198&amp;fLockTerm=Pain+assessment">Pain assessment</a>
 (225399009), <a href="/web/20180712194909/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0031809&amp;fLockTerm=Physical+examination">Physical examination</a>
 (302199004), <a href="/web/20180712194909/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0031809&amp;fLockTerm=Physical+examination">Physical examination</a>
 (5880005), <a href="/web/20180712194909/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0031809&amp;fLockTerm=Physical+examination">Physical examination</a>
 (81375008), <a href="/web/20180712194909/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C1962945&amp;fLockTerm=Radiographic+imaging+procedure">Radiographic imaging procedure</a>
 (363680008), <a href="/web/20180712194909/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0182605&amp;fLockTerm=Radiographic+unit">Radiographic unit</a>
 (68080007), <a href="/web/20180712194909/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0038009&amp;fLockTerm=Splint">Splint</a>
 (16650009), <a href="/web/20180712194909/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0565512&amp;fLockTerm=Splinting+of+fracture">Splinting of fracture</a>
 (288144001), <a href="/web/20180712194909/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0347788&amp;fLockTerm=Supracondylar+fracture+of+humerus">Supracondylar fracture of humerus</a>
 (263193000), <a href="/web/20180712194909/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0886052&amp;fLockTerm=Wound+care">Wound care</a>
 (225358003)                    </div>
