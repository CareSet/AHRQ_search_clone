 (126189002), <a href="/web/20180712202952/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0011701&amp;fLockTerm=Desmopressin">Desmopressin</a>
 (73454001), <a href="/web/20180712202952/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0795575&amp;fLockTerm=Eptacog+alfa+activated">Eptacog alfa activated</a>
 (116066006), <a href="/web/20180712202952/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0795575&amp;fLockTerm=Eptacog+alfa+activated">Eptacog alfa activated</a>
 (428186003), <a href="/web/20180712202952/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0398341&amp;fLockTerm=Extracorporeal+immunoadsorption">Extracorporeal immunoadsorption</a>
 (233593000), <a href="/web/20180712202952/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0015506&amp;fLockTerm=Factor+VIII">Factor VIII</a>
 (278910002), <a href="/web/20180712202952/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0015506&amp;fLockTerm=Factor+VIII">Factor VIII</a>
 (416523005), <a href="/web/20180712202952/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C3494187&amp;fLockTerm=Factor+VIII+deficiency">Factor VIII deficiency</a>
 (234440005), <a href="/web/20180712202952/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C2732879&amp;fLockTerm=Factor+VIII+mutation+carrier+detection+test">Factor VIII mutation carrier detection test</a>
 (443889002), <a href="/web/20180712202952/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C1294200&amp;fLockTerm=Gene+mutation+analysis">Gene mutation analysis</a>
 (118117001), <a href="/web/20180712202952/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0679560&amp;fLockTerm=Genetic+test">Genetic test</a>
 (405824009), <a href="/web/20180712202952/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0008533&amp;fLockTerm=Hereditary+factor+IX+deficiency+disease">Hereditary factor IX deficiency disease</a>
 (41788008), <a href="/web/20180712202952/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0019069&amp;fLockTerm=Hereditary+factor+VIII+deficiency+disease">Hereditary factor VIII deficiency disease</a>
 (28293008), <a href="/web/20180712202952/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0021081&amp;fLockTerm=Immunosuppressant">Immunosuppressant</a>
 (372823004), <a href="/web/20180712202952/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0021081&amp;fLockTerm=Immunosuppressant">Immunosuppressant</a>
 (69431002), <a href="/web/20180712202952/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0021079&amp;fLockTerm=Immunosuppressive+therapy">Immunosuppressive therapy</a>
 (86553008), <a href="/web/20180712202952/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0272324&amp;fLockTerm=Mild+hereditary+factor+VIII+deficiency+disease">Mild hereditary factor VIII deficiency disease</a>
 (26029002), <a href="/web/20180712202952/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0272323&amp;fLockTerm=Moderate+hereditary+factor+VIII+deficiency+disease">Moderate hereditary factor VIII deficiency disease</a>
 (33344008), <a href="/web/20180712202952/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0032134&amp;fLockTerm=Plasmapheresis">Plasmapheresis</a>
 (20720000), <a href="/web/20180712202952/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0086930&amp;fLockTerm=Risk+assessment">Risk assessment</a>
 (225338004), <a href="/web/20180712202952/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0393022&amp;fLockTerm=Rituximab">Rituximab</a>
 (108809004), <a href="/web/20180712202952/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0393022&amp;fLockTerm=Rituximab">Rituximab</a>
 (386919002), <a href="/web/20180712202952/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0272322&amp;fLockTerm=Severe+hereditary+factor+VIII+deficiency+disease">Severe hereditary factor VIII deficiency disease</a>
 (16872008), <a href="/web/20180712202952/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0543467&amp;fLockTerm=Surgical">Surgical</a>
 (257556004), <a href="/web/20180712202952/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0543467&amp;fLockTerm=Surgical">Surgical</a>
 (387713003), <a href="/web/20180712202952/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0543467&amp;fLockTerm=Surgical">Surgical</a>
 (83578000), <a href="/web/20180712202952/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0040613&amp;fLockTerm=Tranexamic+acid">Tranexamic acid</a>
 (109006003), <a href="/web/20180712202952/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0040613&amp;fLockTerm=Tranexamic+acid">Tranexamic acid</a>
 (386960009)                    </div>
                    <!-- /.field-content -->
                </div>
