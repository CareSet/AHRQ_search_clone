 (1449002), <a href="/web/20180712202232/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0018504&amp;fLockTerm=General+hair+removal+-+epilation">General hair removal - epilation</a>
 (287608005), <a href="/web/20180712202232/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0152277&amp;fLockTerm=Incision+AND+drainage">Incision AND drainage</a>
 (56783008), <a href="/web/20180712202232/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0441029&amp;fLockTerm=Limberg+flap">Limberg flap</a>
 (256856002), <a href="/web/20180712202232/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0585527&amp;fLockTerm=Marsupialization+of+pilonidal+sinus">Marsupialization of pilonidal sinus</a>
 (307664006), <a href="/web/20180712202232/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0150706&amp;fLockTerm=Perioperative+care">Perioperative care</a>
 (133897009), <a href="/web/20180712202232/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0070570&amp;fLockTerm=Phenol">Phenol</a>
 (116613007), <a href="/web/20180712202232/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0070570&amp;fLockTerm=Phenol">Phenol</a>
 (354036003), <a href="/web/20180712202232/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0031809&amp;fLockTerm=Physical+examination">Physical examination</a>
 (302199004), <a href="/web/20180712202232/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0031809&amp;fLockTerm=Physical+examination">Physical examination</a>
 (5880005), <a href="/web/20180712202232/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0031809&amp;fLockTerm=Physical+examination">Physical examination</a>
 (81375008), <a href="/web/20180712202232/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0520556&amp;fLockTerm=Pilonidal+cyst+without+abscess">Pilonidal cyst without abscess</a>
 (76545008), <a href="/web/20180712202232/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C2317114&amp;fLockTerm=Pilonidal+disease">Pilonidal disease</a>
 (432863009), <a href="/web/20180712202232/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0032786&amp;fLockTerm=Postoperative+care">Postoperative care</a>
 (133899007), <a href="/web/20180712202232/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0555870&amp;fLockTerm=Rhomboid+flap">Rhomboid flap</a>
 (309833001), <a href="/web/20180712202232/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0522772&amp;fLockTerm=Shaving+-+action">Shaving - action</a>
 (129322004), <a href="/web/20180712202232/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0522772&amp;fLockTerm=Shaving+-+action">Shaving - action</a>
 (29923002), <a href="/web/20180712202232/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0543467&amp;fLockTerm=Surgical">Surgical</a>
 (257556004), <a href="/web/20180712202232/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0543467&amp;fLockTerm=Surgical">Surgical</a>
 (387713003), <a href="/web/20180712202232/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0543467&amp;fLockTerm=Surgical">Surgical</a>
 (83578000), <a href="/web/20180712202232/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0001566&amp;fLockTerm=Topical+administration+of+treatment">Topical administration of treatment</a>
 (419408001), <a href="/web/20180712202232/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0441479&amp;fLockTerm=V-Y+advancement+technique">V-Y advancement technique</a>
 (257740006), <a href="/web/20180712202232/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0729297&amp;fLockTerm=Z-plasty+technique">Z-plasty technique</a>
 (313202008)                    </div>
                    <!-- /.field-content -->
                </div>
