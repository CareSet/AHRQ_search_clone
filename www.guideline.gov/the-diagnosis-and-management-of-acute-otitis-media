 (396425006), <a href="/web/20180712202159/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0021403&amp;fLockTerm=Influenza+virus+vaccine">Influenza virus vaccine</a>
 (46233009), <a href="/web/20180712202159/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0029882&amp;fLockTerm=Otitis+media">Otitis media</a>
 (65363002), <a href="/web/20180712202159/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0030198&amp;fLockTerm=Pain+assessment">Pain assessment</a>
 (225399009), <a href="/web/20180712202159/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0700325&amp;fLockTerm=Patient+status+observation">Patient status observation</a>
 (103705002), <a href="/web/20180712202159/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0358314&amp;fLockTerm=Pneumococcal+vaccine">Pneumococcal vaccine</a>
 (333598008), <a href="/web/20180712202159/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0358314&amp;fLockTerm=Pneumococcal+vaccine">Pneumococcal vaccine</a>
 (398730001), <a href="/web/20180712202159/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0029888&amp;fLockTerm=Purulent+otitis+media">Purulent otitis media</a>
 (39288006), <a href="/web/20180712202159/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0087123&amp;fLockTerm=Tympanostomy">Tympanostomy</a>
 (397760009), <a href="/web/20180712202159/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0087123&amp;fLockTerm=Tympanostomy">Tympanostomy</a>
 (397862008)                    </div>
                    <!-- /.field-content -->
                </div>
                <!-- /.field -->
                <div class="field">
                    <h5 class="field-label">SPN</h5>
                    <div class="field-content">
<a href="/web/20180712202159/https://www.guideline.gov/search?f_SPN_CUI=C0184147&amp;fLockTerm=TUBE%2c+TYMPANOSTOMY">TUBE, TYMPANOSTOMY</a>
                    </div>
                    <!-- /.field-content -->
                </div>
                <!-- /.field -->
                <div class="field">
                    <h5 class="field-label">UMD</h5>
                    <div class="field-content">
<a href="/web/20180712202159/https://www.guideline.gov/search?f_UMD_CUI=C0184147&amp;fLockTerm=Tubes%2c+Middle+Ear%2c+Myringotomy">Tubes, Middle Ear, Myringotomy</a>
 (15-323)                    </div>
                    <!-- /.field-content -->
                </div>
                <!-- /.field -->
    </div>
