<a href="/web/20180713025345/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0496854&amp;fLockTerm=Carcinoma+in+situ+of+liver+and%2for+biliary+system">Carcinoma in situ of liver and/or biliary system</a>
 (271525004), <a href="/web/20180713025345/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0441474&amp;fLockTerm=Chemical+destruction">Chemical destruction</a>
 (257730008), <a href="/web/20180713025345/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C3665472&amp;fLockTerm=Chemotherapy">Chemotherapy</a>
 (363688001), <a href="/web/20180713025345/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C3665472&amp;fLockTerm=Chemotherapy">Chemotherapy</a>
 (367336001), <a href="/web/20180713025345/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0206698&amp;fLockTerm=Cholangiocarcinoma">Cholangiocarcinoma</a>
 (312104005), <a href="/web/20180713025345/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0206698&amp;fLockTerm=Cholangiocarcinoma">Cholangiocarcinoma</a>
 (70179006), <a href="/web/20180713025345/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0013216&amp;fLockTerm=Drug+therapy">Drug therapy</a>
 (416608005), <a href="/web/20180713025345/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0013931&amp;fLockTerm=Embolization+procedure">Embolization procedure</a>
 (122463005), <a href="/web/20180713025345/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0013931&amp;fLockTerm=Embolization+procedure">Embolization procedure</a>
 (129335008), <a href="/web/20180713025345/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C2239176&amp;fLockTerm=Hepatocellular+carcinoma">Hepatocellular carcinoma</a>
 (109841003), <a href="/web/20180713025345/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C2239176&amp;fLockTerm=Hepatocellular+carcinoma">Hepatocellular carcinoma</a>
 (187769009), <a href="/web/20180713025345/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C2239176&amp;fLockTerm=Hepatocellular+carcinoma">Hepatocellular carcinoma</a>
 (25370001), <a href="/web/20180713025345/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0019144&amp;fLockTerm=Liver+excision">Liver excision</a>
 (107963000), <a href="/web/20180713025345/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0345904&amp;fLockTerm=Malignant+neoplasm+of+liver">Malignant neoplasm of liver</a>
 (93870000), <a href="/web/20180713025345/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0009404&amp;fLockTerm=Neoplasm+of+large+intestine">Neoplasm of large intestine</a>
 (126837005), <a href="/web/20180713025345/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0023903&amp;fLockTerm=Neoplasm+of+liver">Neoplasm of liver</a>
 (126851005), <a href="/web/20180713025345/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0206754&amp;fLockTerm=Neuroendocrine+tumor">Neuroendocrine tumor</a>
 (128928004), <a href="/web/20180713025345/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0206754&amp;fLockTerm=Neuroendocrine+tumor">Neuroendocrine tumor</a>
 (255046005), <a href="/web/20180713025345/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0028833&amp;fLockTerm=Octreotide">Octreotide</a>
 (109053000), <a href="/web/20180713025345/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0028833&amp;fLockTerm=Octreotide">Octreotide</a>
 (126155003), <a href="/web/20180713025345/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0038898&amp;fLockTerm=Operative+procedure+on+digestive+system">Operative procedure on digestive system</a>
 (107907001), <a href="/web/20180713025345/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C1960012&amp;fLockTerm=Percutaneous+chemical+ablation+of+lesion+of+liver">Percutaneous chemical ablation of lesion of liver</a>
 (426713000), <a href="/web/20180713025345/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C1522449&amp;fLockTerm=Radiation+oncology+AND%2fOR+radiotherapy">Radiation oncology AND/OR radiotherapy</a>
 (108290001), <a href="/web/20180713025345/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C1522449&amp;fLockTerm=Radiation+oncology+AND%2fOR+radiotherapy">Radiation oncology AND/OR radiotherapy</a>
 (53438000), <a href="/web/20180713025345/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0302617&amp;fLockTerm=Radiographic-therapeutic+unit">Radiographic-therapeutic unit</a>
 (10850003), <a href="/web/20180713025345/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0494165&amp;fLockTerm=Secondary+malignant+neoplasm+of+liver">Secondary malignant neoplasm of liver</a>
 (94381002), <a href="/web/20180713025345/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0023911&amp;fLockTerm=Transplantation+of+liver">Transplantation of liver</a>
 (18027006)                    </div>
