 (314716005), <a href="/web/20180712204238/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0040405&amp;fLockTerm=Computerized+axial+tomography">Computerized axial tomography</a>
 (77477000), <a href="/web/20180712204238/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0009924&amp;fLockTerm=Contrast+media">Contrast media</a>
 (385420005), <a href="/web/20180712204238/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0009924&amp;fLockTerm=Contrast+media">Contrast media</a>
 (407935004), <a href="/web/20180712204238/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0011906&amp;fLockTerm=Differential+diagnosis">Differential diagnosis</a>
 (47965005), <a href="/web/20180712204238/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0242845&amp;fLockTerm=Duplex+ultrasound">Duplex ultrasound</a>
 (312236008), <a href="/web/20180712204238/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0032987&amp;fLockTerm=Ectopic+pregnancy">Ectopic pregnancy</a>
 (34801009), <a href="/web/20180712204238/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0242172&amp;fLockTerm=Female+pelvic+inflammatory+disease">Female pelvic inflammatory disease</a>
 (198130006), <a href="/web/20180712204238/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0106132&amp;fLockTerm=Human+chorionic+gonadotropin%2c+beta+subunit">Human chorionic gonadotropin, beta subunit</a>
 (40940006), <a href="/web/20180712204238/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0011923&amp;fLockTerm=Imaging">Imaging</a>
 (363679005), <a href="/web/20180712204238/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0024485&amp;fLockTerm=Magnetic+resonance+imaging">Magnetic resonance imaging</a>
 (113091000), <a href="/web/20180712204238/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0024485&amp;fLockTerm=Magnetic+resonance+imaging">Magnetic resonance imaging</a>
 (312250003), <a href="/web/20180712204238/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0336660&amp;fLockTerm=Magnetic+resonance+imaging+unit">Magnetic resonance imaging unit</a>
 (90003000), <a href="/web/20180712204238/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0030794&amp;fLockTerm=Pain+in+pelvis">Pain in pelvis</a>
 (30473006), <a href="/web/20180712204238/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0032961&amp;fLockTerm=Pregnancy">Pregnancy</a>
