 (413575009), <a href="/web/20180712201356/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0027646&amp;fLockTerm=Cancer+staging">Cancer staging</a>
 (258232002), <a href="/web/20180712201356/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0686288&amp;fLockTerm=Carcinoma+in+situ+of+female+breast">Carcinoma in situ of female breast</a>
 (92593006), <a href="/web/20180712201356/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0007124&amp;fLockTerm=Intraductal+carcinoma%2c+noninfiltrating">Intraductal carcinoma, noninfiltrating</a>
 (109889007), <a href="/web/20180712201356/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0007124&amp;fLockTerm=Intraductal+carcinoma%2c+noninfiltrating">Intraductal carcinoma, noninfiltrating</a>
 (278053004), <a href="/web/20180712201356/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0007124&amp;fLockTerm=Intraductal+carcinoma%2c+noninfiltrating">Intraductal carcinoma, noninfiltrating</a>
 (373176000), <a href="/web/20180712201356/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0007124&amp;fLockTerm=Intraductal+carcinoma%2c+noninfiltrating">Intraductal carcinoma, noninfiltrating</a>
 (86616005), <a href="/web/20180712201356/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0851238&amp;fLockTerm=Lumpectomy+of+breast">Lumpectomy of breast</a>
 (392021009), <a href="/web/20180712201356/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0851238&amp;fLockTerm=Lumpectomy+of+breast">Lumpectomy of breast</a>
 (392022002), <a href="/web/20180712201356/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0024485&amp;fLockTerm=Magnetic+resonance+imaging">Magnetic resonance imaging</a>
 (113091000), <a href="/web/20180712201356/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0024485&amp;fLockTerm=Magnetic+resonance+imaging">Magnetic resonance imaging</a>
 (312250003), <a href="/web/20180712201356/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0336660&amp;fLockTerm=Magnetic+resonance+imaging+unit">Magnetic resonance imaging unit</a>
 (90003000), <a href="/web/20180712201356/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0235653&amp;fLockTerm=Malignant+neoplasm+of+female+breast">Malignant neoplasm of female breast</a>
 (372064008), <a href="/web/20180712201356/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0006142&amp;fLockTerm=Malignant+tumor+of+breast">Malignant tumor of breast</a>
 (254837009), <a href="/web/20180712201356/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C1458155&amp;fLockTerm=Neoplasm+of+breast">Neoplasm of breast</a>
 (126926005), <a href="/web/20180712201356/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0024885&amp;fLockTerm=Partial+mastectomy">Partial mastectomy</a>
 (237370008), <a href="/web/20180712201356/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0024885&amp;fLockTerm=Partial+mastectomy">Partial mastectomy</a>
 (64368001), <a href="/web/20180712201356/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0700325&amp;fLockTerm=Patient+status+observation">Patient status observation</a>
 (103705002), <a href="/web/20180712201356/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C1522449&amp;fLockTerm=Radiation+oncology+AND%2fOR+radiotherapy">Radiation oncology AND/OR radiotherapy</a>
 (108290001), <a href="/web/20180712201356/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C1522449&amp;fLockTerm=Radiation+oncology+AND%2fOR+radiotherapy">Radiation oncology AND/OR radiotherapy</a>
 (53438000), <a href="/web/20180712201356/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0302617&amp;fLockTerm=Radiographic-therapeutic+unit">Radiographic-therapeutic unit</a>
 (10850003), <a href="/web/20180712201356/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0796693&amp;fLockTerm=Sentinel+lymph+node+biopsy">Sentinel lymph node biopsy</a>
 (396487001), <a href="/web/20180712201356/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0039286&amp;fLockTerm=Tamoxifen">Tamoxifen</a>
 (373345002), <a href="/web/20180712201356/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0039286&amp;fLockTerm=Tamoxifen">Tamoxifen</a>
 (75959001), <a href="/web/20180712201356/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0728747&amp;fLockTerm=Trastuzumab">Trastuzumab</a>
 (327397006), <a href="/web/20180712201356/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0728747&amp;fLockTerm=Trastuzumab">Trastuzumab</a>
 (387003001)                    </div>
