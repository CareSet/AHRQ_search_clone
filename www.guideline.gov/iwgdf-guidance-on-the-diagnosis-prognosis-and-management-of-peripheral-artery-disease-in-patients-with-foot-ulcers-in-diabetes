 (108979001), <a href="/web/20180712203718/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0070166&amp;fLockTerm=Clopidogrel">Clopidogrel</a>
 (386952008), <a href="/web/20180712203718/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C1536105&amp;fLockTerm=CT+angiography">CT angiography</a>
 (418272005), <a href="/web/20180712203718/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0033325&amp;fLockTerm=Determination+of+prognosis">Determination of prognosis</a>
 (20481000), <a href="/web/20180712203718/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0011849&amp;fLockTerm=Diabetes+mellitus">Diabetes mellitus</a>
 (73211009), <a href="/web/20180712203718/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0342257&amp;fLockTerm=Diabetic+complication">Diabetic complication</a>
 (74627003), <a href="/web/20180712203718/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0206172&amp;fLockTerm=Diabetic+foot">Diabetic foot</a>
 (280137006), <a href="/web/20180712203718/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C1456868&amp;fLockTerm=Diabetic+foot+ulcer">Diabetic foot ulcer</a>
 (371087003), <a href="/web/20180712203718/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C4032653&amp;fLockTerm=Diabetic+foot+ulcer+prevention">Diabetic foot ulcer prevention</a>
 (713150007), <a href="/web/20180712203718/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0002979&amp;fLockTerm=Digital+subtraction+angiography">Digital subtraction angiography</a>
 (312246005), <a href="/web/20180712203718/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0242845&amp;fLockTerm=Duplex+ultrasound">Duplex ultrasound</a>
 (312236008), <a href="/web/20180712203718/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0232157&amp;fLockTerm=Foot+pulse%2c+function">Foot pulse, function</a>
 (422121001), <a href="/web/20180712203718/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0360714&amp;fLockTerm=HMG-CoA+reductase+inhibitor">HMG-CoA reductase inhibitor</a>
 (372912004), <a href="/web/20180712203718/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0360714&amp;fLockTerm=HMG-CoA+reductase+inhibitor">HMG-CoA reductase inhibitor</a>
 (96302009), <a href="/web/20180712203718/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0020616&amp;fLockTerm=Hypoglycemic+agent">Hypoglycemic agent</a>
 (312064005), <a href="/web/20180712203718/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0473558&amp;fLockTerm=Ischemic+foot+ulcer">Ischemic foot ulcer</a>
 (238794007), <a href="/web/20180712203718/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0243032&amp;fLockTerm=Magnetic+resonance+imaging+(MRI)+of+vessels">Magnetic resonance imaging (MRI) of vessels</a>
 (241663008), <a href="/web/20180712203718/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0336660&amp;fLockTerm=Magnetic+resonance+imaging+unit">Magnetic resonance imaging unit</a>
 (90003000), <a href="/web/20180712203718/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0428173&amp;fLockTerm=Measurement+of+transcutaneous+partial+pressure+of+oxygen">Measurement of transcutaneous partial pressure of oxygen</a>
 (250549007), <a href="/web/20180712203718/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0581603&amp;fLockTerm=Revascularization+-+action">Revascularization - action</a>
 (297183000), <a href="/web/20180712203718/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0472672&amp;fLockTerm=Revascularization+of+foot">Revascularization of foot</a>
 (233507005), <a href="/web/20180712203718/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0085119&amp;fLockTerm=Ulcer+of+foot">Ulcer of foot</a>
 (95345008)                    </div>
                    <!-- /.field-content -->
                </div>
                <!-- /.field -->
