 (609588000), <a href="/web/20180712203906/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0585707&amp;fLockTerm=Arthroplasty+of+knee+using+cement">Arthroplasty of knee using cement</a>
 (307817008), <a href="/web/20180712203906/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0585719&amp;fLockTerm=Arthroplasty+of+knee+without+cement">Arthroplasty of knee without cement</a>
 (307819006), <a href="/web/20180712203906/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0005934&amp;fLockTerm=Bone+cement">Bone cement</a>
 (356497001), <a href="/web/20180712203906/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0005934&amp;fLockTerm=Bone+cement">Bone cement</a>
 (6043006), <a href="/web/20180712203906/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0600577&amp;fLockTerm=Chiropractic+manipulation">Chiropractic manipulation</a>
 (46947000), <a href="/web/20180712203906/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0149871&amp;fLockTerm=Deep+venous+thrombosis">Deep venous thrombosis</a>
 (128053003), <a href="/web/20180712203906/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0002913&amp;fLockTerm=Epidural+anesthesia">Epidural anesthesia</a>
 (18946005), <a href="/web/20180712203906/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0079027&amp;fLockTerm=Intraoperative+hemorrhage">Intraoperative hemorrhage</a>
 (213261000), <a href="/web/20180712203906/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0022748&amp;fLockTerm=Knee+joint+prosthesis">Knee joint prosthesis</a>
 (109228008), <a href="/web/20180712203906/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0002921&amp;fLockTerm=Local+anesthesia">Local anesthesia</a>
 (386761002), <a href="/web/20180712203906/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0498269&amp;fLockTerm=Manipulation+under+anesthesia">Manipulation under anesthesia</a>
 (274470005), <a href="/web/20180712203906/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0027741&amp;fLockTerm=Nerve+block">Nerve block</a>
 (56333001), <a href="/web/20180712203906/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0027741&amp;fLockTerm=Nerve+block">Nerve block</a>
 (64874008), <a href="/web/20180712203906/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0409959&amp;fLockTerm=Osteoarthritis+of+knee">Osteoarthritis of knee</a>
 (239873007), <a href="/web/20180712203906/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0029468&amp;fLockTerm=Osteotomy">Osteotomy</a>
 (150062003), <a href="/web/20180712203906/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0029468&amp;fLockTerm=Osteotomy">Osteotomy</a>
 (299713009), <a href="/web/20180712203906/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0029468&amp;fLockTerm=Osteotomy">Osteotomy</a>
 (79881004), <a href="/web/20180712203906/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0949766&amp;fLockTerm=Physical+therapy+procedure">Physical therapy procedure</a>
 (91251008), <a href="/web/20180712203906/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0033061&amp;fLockTerm=Preoperative+care">Preoperative care</a>
 (133898004), <a href="/web/20180712203906/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0408429&amp;fLockTerm=Resurfacing+of+the+patella">Resurfacing of the patella</a>
 (239503002), <a href="/web/20180712203906/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0035648&amp;fLockTerm=Risk+factor">Risk factor</a>
 (80943009), <a href="/web/20180712203906/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0452240&amp;fLockTerm=Therapeutic+exercise">Therapeutic exercise</a>
 (226029000), <a href="/web/20180712203906/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0452240&amp;fLockTerm=Therapeutic+exercise">Therapeutic exercise</a>
 (229065009), <a href="/web/20180712203906/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0452240&amp;fLockTerm=Therapeutic+exercise">Therapeutic exercise</a>
 (51998003), <a href="/web/20180712203906/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0040519&amp;fLockTerm=Tourniquet">Tourniquet</a>
 (38141007), <a href="/web/20180712203906/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0040613&amp;fLockTerm=Tranexamic+acid">Tranexamic acid</a>
 (109006003), <a href="/web/20180712203906/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0040613&amp;fLockTerm=Tranexamic+acid">Tranexamic acid</a>
 (386960009), <a href="/web/20180712203906/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0042487&amp;fLockTerm=Venous+thrombosis">Venous thrombosis</a>
 (111293003)                    </div>
                    <!-- /.field-content -->
                </div>
                <!-- /.field -->
                <div class="field">
                    <h5 class="field-label">SPN</h5>
                    <div class="field-content">
<a href="/web/20180712203906/https://www.guideline.gov/search?f_SPN_CUI=C0005934&amp;fLockTerm=BONE+CEMENT">BONE CEMENT</a>
                    </div>
