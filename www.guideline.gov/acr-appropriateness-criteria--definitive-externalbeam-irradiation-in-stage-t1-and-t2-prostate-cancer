 (346324007), <a href="/web/20180713052516/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0006098&amp;fLockTerm=Brachytherapy+-+action">Brachytherapy - action</a>
 (313029009), <a href="/web/20180713052516/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C1139859&amp;fLockTerm=Brachytherapy+system">Brachytherapy system</a>
 (706279005), <a href="/web/20180713052516/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0040405&amp;fLockTerm=Computerized+axial+tomography">Computerized axial tomography</a>
 (77477000), <a href="/web/20180713052516/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0729707&amp;fLockTerm=Computerized+tomography%2c+3+dimensional+reconstruction">Computerized tomography, 3 dimensional reconstruction</a>
 (22400007), <a href="/web/20180713052516/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0010408&amp;fLockTerm=Cryosurgery">Cryosurgery</a>
 (129393004), <a href="/web/20180713052516/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0010408&amp;fLockTerm=Cryosurgery">Cryosurgery</a>
 (281609006), <a href="/web/20180713052516/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0010408&amp;fLockTerm=Cryosurgery">Cryosurgery</a>
 (281698007), <a href="/web/20180713052516/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0010408&amp;fLockTerm=Cryosurgery">Cryosurgery</a>
 (427357007), <a href="/web/20180713052516/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C1512814&amp;fLockTerm=Intensity+modulated+radiation+therapy">Intensity modulated radiation therapy</a>
 (441799006), <a href="/web/20180713052516/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0376358&amp;fLockTerm=Malignant+tumor+of+prostate">Malignant tumor of prostate</a>
 (399068003), <a href="/web/20180713052516/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0033578&amp;fLockTerm=Neoplasm+of+prostate">Neoplasm of prostate</a>
 (126906006), <a href="/web/20180713052516/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0033573&amp;fLockTerm=Prostatectomy">Prostatectomy</a>
 (90470006), <a href="/web/20180713052516/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0436226&amp;fLockTerm=Proton+therapy">Proton therapy</a>
 (169314007), <a href="/web/20180713052516/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C1522449&amp;fLockTerm=Radiation+oncology+AND%2fOR+radiotherapy">Radiation oncology AND/OR radiotherapy</a>
 (108290001), <a href="/web/20180713052516/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C1522449&amp;fLockTerm=Radiation+oncology+AND%2fOR+radiotherapy">Radiation oncology AND/OR radiotherapy</a>
 (53438000), <a href="/web/20180713052516/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0194810&amp;fLockTerm=Radical+prostatectomy">Radical prostatectomy</a>
 (26294005), <a href="/web/20180713052516/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0302617&amp;fLockTerm=Radiographic-therapeutic+unit">Radiographic-therapeutic unit</a>
 (10850003), <a href="/web/20180713052516/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C1276484&amp;fLockTerm=T1%3a+Clinically+inapparent+prostate+tumor+not+palpable+or+visible+by+imaging">T1: Clinically inapparent prostate tumor not palpable or visible by imaging</a>
 (369832002), <a href="/web/20180713052516/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C1276626&amp;fLockTerm=T2%3a+Tumor+confined+within+the+prostate">T2: Tumor confined within the prostate</a>
