 (387426003), <a href="/web/20180609222341/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0050451&amp;fLockTerm=Acetohydroxamic+acid">Acetohydroxamic acid</a>
 (74819009), <a href="/web/20180609222341/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0002144&amp;fLockTerm=Allopurinol">Allopurinol</a>
 (25246002), <a href="/web/20180609222341/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0002144&amp;fLockTerm=Allopurinol">Allopurinol</a>
 (387135004), <a href="/web/20180609222341/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0005774&amp;fLockTerm=Blood+chemistry">Blood chemistry</a>
 (166312007), <a href="/web/20180609222341/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0489458&amp;fLockTerm=Calcium+intake">Calcium intake</a>
 (230122008), <a href="/web/20180609222341/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0425401&amp;fLockTerm=Dietary+history">Dietary history</a>
 (162512009), <a href="/web/20180609222341/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0012798&amp;fLockTerm=Diuretic">Diuretic</a>
 (30492008), <a href="/web/20180609222341/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0012798&amp;fLockTerm=Diuretic">Diuretic</a>
 (372695000), <a href="/web/20180609222341/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0589120&amp;fLockTerm=Follow-up+status">Follow-up status</a>
 (308273005), <a href="/web/20180609222341/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0016767&amp;fLockTerm=Fruit">Fruit</a>
 (72511004), <a href="/web/20180609222341/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0556227&amp;fLockTerm=Fruit+intake">Fruit intake</a>
 (226452008), <a href="/web/20180609222341/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0011923&amp;fLockTerm=Imaging">Imaging</a>
 (363679005), <a href="/web/20180609222341/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C3661562&amp;fLockTerm=Increased+fluid+diet">Increased fluid diet</a>
 (439071000124106), <a href="/web/20180609222341/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0022650&amp;fLockTerm=Kidney+stone">Kidney stone</a>
 (95570007), <a href="/web/20180609222341/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0201821&amp;fLockTerm=Kidney+stone+analysis">Kidney stone analysis</a>
 (16620001), <a href="/web/20180609222341/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0242972&amp;fLockTerm=Low+protein+diet">Low protein diet</a>
 (160673009), <a href="/web/20180609222341/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0242972&amp;fLockTerm=Low+protein+diet">Low protein diet</a>
 (437471000124106), <a href="/web/20180609222341/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0012169&amp;fLockTerm=Low+sodium+diet">Low sodium diet</a>
 (386619000), <a href="/web/20180609222341/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0012169&amp;fLockTerm=Low+sodium+diet">Low sodium diet</a>
 (437421000124105), <a href="/web/20180609222341/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C1273415&amp;fLockTerm=Metabolic+screening+test">Metabolic screening test</a>
 (395118002), <a href="/web/20180609222341/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0137996&amp;fLockTerm=Potassium+citrate">Potassium citrate</a>
