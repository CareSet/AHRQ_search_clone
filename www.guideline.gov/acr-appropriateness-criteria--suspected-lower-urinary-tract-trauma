 (353687008), <a href="/web/20180713043837/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0303611&amp;fLockTerm=%5e99m%5eTechnetium">^99m^Technetium</a>
 (72454006), <a href="/web/20180713043837/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0002978&amp;fLockTerm=Angiography">Angiography</a>
 (77343006), <a href="/web/20180713043837/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0003844&amp;fLockTerm=Arteriography">Arteriography</a>
 (129118002), <a href="/web/20180713043837/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0018965&amp;fLockTerm=Blood+in+urine">Blood in urine</a>
 (34436003), <a href="/web/20180713043837/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0018965&amp;fLockTerm=Blood+in+urine">Blood in urine</a>
 (53298000), <a href="/web/20180713043837/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0040405&amp;fLockTerm=Computerized+axial+tomography">Computerized axial tomography</a>
 (77477000), <a href="/web/20180713043837/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0009924&amp;fLockTerm=Contrast+media">Contrast media</a>
 (385420005), <a href="/web/20180713043837/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0009924&amp;fLockTerm=Contrast+media">Contrast media</a>
 (407935004), <a href="/web/20180713043837/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0203126&amp;fLockTerm=Cystography">Cystography</a>
 (168854009), <a href="/web/20180713043837/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0203126&amp;fLockTerm=Cystography">Cystography</a>
 (418135007), <a href="/web/20180713043837/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0034573&amp;fLockTerm=Diagnostic+radiography+of+abdomen">Diagnostic radiography of abdomen</a>
 (60654006), <a href="/web/20180713043837/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0013931&amp;fLockTerm=Embolization+procedure">Embolization procedure</a>
 (122463005), <a href="/web/20180713043837/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0013931&amp;fLockTerm=Embolization+procedure">Embolization procedure</a>
 (129335008), <a href="/web/20180713043837/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0848377&amp;fLockTerm=Injury+of+abdomen">Injury of abdomen</a>
 (128069005), <a href="/web/20180713043837/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0403677&amp;fLockTerm=Injury+of+bladder">Injury of bladder</a>
 (77165001), <a href="/web/20180713043837/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0238493&amp;fLockTerm=Injury+of+ureter">Injury of ureter</a>
 (24850009), <a href="/web/20180713043837/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0403701&amp;fLockTerm=Injury+of+urethra">Injury of urethra</a>
 (22724000), <a href="/web/20180713043837/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0497561&amp;fLockTerm=Injury+of+urinary+tract">Injury of urinary tract</a>
 (262890007), <a href="/web/20180713043837/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0203108&amp;fLockTerm=Intravenous+pyelogram">Intravenous pyelogram</a>
 (32265006), <a href="/web/20180713043837/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0024485&amp;fLockTerm=Magnetic+resonance+imaging">Magnetic resonance imaging</a>
 (113091000), <a href="/web/20180713043837/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0024485&amp;fLockTerm=Magnetic+resonance+imaging">Magnetic resonance imaging</a>
 (312250003), <a href="/web/20180713043837/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0336660&amp;fLockTerm=Magnetic+resonance+imaging+unit">Magnetic resonance imaging unit</a>
 (90003000), <a href="/web/20180713043837/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0034606&amp;fLockTerm=Nuclear+medicine+imaging+procedure">Nuclear medicine imaging procedure</a>
 (373205008), <a href="/web/20180713043837/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C1962945&amp;fLockTerm=Radiographic+imaging+procedure">Radiographic imaging procedure</a>
 (363680008), <a href="/web/20180713043837/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0412155&amp;fLockTerm=Retrograde+cystography">Retrograde cystography</a>
 (168858007), <a href="/web/20180713043837/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0206057&amp;fLockTerm=Technetium+compound">Technetium compound</a>
 (47379009), <a href="/web/20180713043837/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0041618&amp;fLockTerm=Ultrasonography">Ultrasonography</a>
 (16310003), <a href="/web/20180713043837/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0041618&amp;fLockTerm=Ultrasonography">Ultrasonography</a>
 (278292003), <a href="/web/20180713043837/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0581495&amp;fLockTerm=Urethrogram">Urethrogram</a>
 (168864000)                    </div>
