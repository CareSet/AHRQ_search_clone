 (441171000124109), <a href="/web/20180713025908/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0040405&amp;fLockTerm=Computerized+axial+tomography">Computerized axial tomography</a>
 (77477000), <a href="/web/20180713025908/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0006107&amp;fLockTerm=Concussion+injury+of+brain">Concussion injury of brain</a>
 (110030002), <a href="/web/20180713025908/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0009676&amp;fLockTerm=Confusional+state">Confusional state</a>
 (286933003), <a href="/web/20180713025908/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0010210&amp;fLockTerm=Counseling">Counseling</a>
 (129441002), <a href="/web/20180713025908/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0010210&amp;fLockTerm=Counseling">Counseling</a>
 (409063005), <a href="/web/20180713025908/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0202691&amp;fLockTerm=CT+of+head">CT of head</a>
 (303653007), <a href="/web/20180713025908/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0027853&amp;fLockTerm=Neurological+assessment">Neurological assessment</a>
 (225398001), <a href="/web/20180713025908/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0027853&amp;fLockTerm=Neurological+assessment">Neurological assessment</a>
 (84728005), <a href="/web/20180713025908/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0027902&amp;fLockTerm=Neuropsychological+testing">Neuropsychological testing</a>
 (307808008), <a href="/web/20180713025908/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0035648&amp;fLockTerm=Risk+factor">Risk factor</a>
 (80943009), <a href="/web/20180713025908/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0876926&amp;fLockTerm=Traumatic+brain+injury">Traumatic brain injury</a>
 (127295002), <a href="/web/20180713025908/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0041657&amp;fLockTerm=Unconscious">Unconscious</a>
 (418107008), <a href="/web/20180713025908/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0041657&amp;fLockTerm=Unconscious">Unconscious</a>
 (419045004)                    </div>
                    <!-- /.field-content -->
                </div>
                <!-- /.field -->
