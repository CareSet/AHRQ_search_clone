 (67246004), <a href="/web/20180712210557/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0201933&amp;fLockTerm=Carcinoembryonic+antigen+measurement">Carcinoembryonic antigen measurement</a>
 (60267001), <a href="/web/20180712210557/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0149684&amp;fLockTerm=Colonic+polypectomy">Colonic polypectomy</a>
 (274025005), <a href="/web/20180712210557/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0180022&amp;fLockTerm=Colonoscope">Colonoscope</a>
 (90412006), <a href="/web/20180712210557/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0009378&amp;fLockTerm=Colonoscopy">Colonoscopy</a>
 (73761001), <a href="/web/20180712210557/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0040405&amp;fLockTerm=Computerized+axial+tomography">Computerized axial tomography</a>
 (77477000), <a href="/web/20180712210557/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0333983&amp;fLockTerm=Hyperplastic+polyp">Hyperplastic polyp</a>
 (277163006), <a href="/web/20180712210557/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0333983&amp;fLockTerm=Hyperplastic+polyp">Hyperplastic polyp</a>
 (62047007), <a href="/web/20180712210557/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0007102&amp;fLockTerm=Malignant+tumor+of+colon">Malignant tumor of colon</a>
 (363406005), <a href="/web/20180712210557/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0949022&amp;fLockTerm=Malignant+tumor+of+rectum">Malignant tumor of rectum</a>
 (363351006), <a href="/web/20180712210557/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0009404&amp;fLockTerm=Neoplasm+of+large+intestine">Neoplasm of large intestine</a>
 (126837005), <a href="/web/20180712210557/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0028792&amp;fLockTerm=Occult+blood+screening">Occult blood screening</a>
 (252156002), <a href="/web/20180712210557/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0038898&amp;fLockTerm=Operative+procedure+on+digestive+system">Operative procedure on digestive system</a>
 (107907001), <a href="/web/20180712210557/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0031809&amp;fLockTerm=Physical+examination">Physical examination</a>
 (302199004), <a href="/web/20180712210557/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0031809&amp;fLockTerm=Physical+examination">Physical examination</a>
 (5880005), <a href="/web/20180712210557/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0031809&amp;fLockTerm=Physical+examination">Physical examination</a>
 (81375008), <a href="/web/20180712210557/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0039985&amp;fLockTerm=Plain+chest+X-ray">Plain chest X-ray</a>
 (399208008), <a href="/web/20180712210557/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0009376&amp;fLockTerm=Polyp+of+colon">Polyp of colon</a>
 (68496003), <a href="/web/20180712210557/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0032786&amp;fLockTerm=Postoperative+care">Postoperative care</a>
 (133899007), <a href="/web/20180712210557/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0041618&amp;fLockTerm=Ultrasonography">Ultrasonography</a>
 (16310003), <a href="/web/20180712210557/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0041618&amp;fLockTerm=Ultrasonography">Ultrasonography</a>
 (278292003)                    </div>
                    <!-- /.field-content -->
                </div>
                <!-- /.field -->
