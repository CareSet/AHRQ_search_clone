 (103969000), <a href="/web/20180712205106/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C2349595&amp;fLockTerm=Fetal+anemia">Fetal anemia</a>
 (462166006), <a href="/web/20180712205106/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0270202&amp;fLockTerm=Hemolytic+disease+of+fetus+OR+newborn+due+to+ABO+immunization">Hemolytic disease of fetus OR newborn due to ABO immunization</a>
 (32858009), <a href="/web/20180712205106/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0014761&amp;fLockTerm=Hemolytic+disease+of+fetus+OR+newborn+due+to+isoimmunization">Hemolytic disease of fetus OR newborn due to isoimmunization</a>
 (387705004), <a href="/web/20180712205106/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0158962&amp;fLockTerm=Hemolytic+disease+of+fetus+OR+newborn+due+to+RhD+isoimmunization">Hemolytic disease of fetus OR newborn due to RhD isoimmunization</a>
 (86986002), <a href="/web/20180712205106/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0005843&amp;fLockTerm=Intrauterine+transfusion">Intrauterine transfusion</a>
 (45460008), <a href="/web/20180712205106/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0022144&amp;fLockTerm=Isoantibody">Isoantibody</a>
 (59905008), <a href="/web/20180712205106/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0031765&amp;fLockTerm=Light+therapy">Light therapy</a>
 (31394004), <a href="/web/20180712205106/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0002891&amp;fLockTerm=Neonatal+anemia">Neonatal anemia</a>
 (234350007), <a href="/web/20180712205106/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0857007&amp;fLockTerm=Neonatal+hyperbilirubinemia">Neonatal hyperbilirubinemia</a>
 (281610001), <a href="/web/20180712205106/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0022353&amp;fLockTerm=Neonatal+jaundice">Neonatal jaundice</a>
 (387712008), <a href="/web/20180712205106/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0580325&amp;fLockTerm=Non-rhesus+red+cell+antibodies">Non-rhesus red cell antibodies</a>
 (270926000), <a href="/web/20180712205106/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0199741&amp;fLockTerm=Phototherapy+of+newborn">Phototherapy of newborn</a>
 (35683002), <a href="/web/20180712205106/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0182261&amp;fLockTerm=Phototherapy+unit">Phototherapy unit</a>
 (721109005), <a href="/web/20180712205106/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0032961&amp;fLockTerm=Pregnancy">Pregnancy</a>
 (289908002), <a href="/web/20180712205106/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0073188&amp;fLockTerm=Rh%3eo%3c(D)+immune+globulin">Rh&gt;o&lt;(D) immune globulin</a>
 (49802003), <a href="/web/20180712205106/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0005841&amp;fLockTerm=Transfusion+of+blood+product">Transfusion of blood product</a>
 (116859006)                    </div>
                    <!-- /.field-content -->
                </div>
                <!-- /.field -->
