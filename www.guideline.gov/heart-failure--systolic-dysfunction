 (346312000), <a href="/web/20180712210714/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0002007&amp;fLockTerm=Aldosterone+antagonists">Aldosterone antagonists</a>
 (372603003), <a href="/web/20180712210714/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0003015&amp;fLockTerm=Angiotensin-converting+enzyme+inhibitor">Angiotensin-converting enzyme inhibitor</a>
 (372733002), <a href="/web/20180712210714/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0003015&amp;fLockTerm=Angiotensin-converting+enzyme+inhibitor">Angiotensin-converting enzyme inhibitor</a>
 (41549009), <a href="/web/20180712210714/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0001645&amp;fLockTerm=beta-Blocking+agent">beta-Blocking agent</a>
 (33252009), <a href="/web/20180712210714/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0001645&amp;fLockTerm=beta-Blocking+agent">beta-Blocking agent</a>
 (373254001), <a href="/web/20180712210714/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0005774&amp;fLockTerm=Blood+chemistry">Blood chemistry</a>
 (166312007), <a href="/web/20180712210714/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C1095989&amp;fLockTerm=Brain+natriuretic+peptide+measurement">Brain natriuretic peptide measurement</a>
 (390917008), <a href="/web/20180712210714/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0018803&amp;fLockTerm=Cardiac+function+testing">Cardiac function testing</a>
 (55574009), <a href="/web/20180712210714/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0030163&amp;fLockTerm=Cardiac+pacemaker">Cardiac pacemaker</a>
 (14106009), <a href="/web/20180712210714/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0007220&amp;fLockTerm=Cardiovascular+agent">Cardiovascular agent</a>
 (373247007), <a href="/web/20180712210714/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0018802&amp;fLockTerm=Congestive+heart+failure">Congestive heart failure</a>
 (42343007), <a href="/web/20180712210714/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0012265&amp;fLockTerm=Digoxin">Digoxin</a>
 (387461009), <a href="/web/20180712210714/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0012265&amp;fLockTerm=Digoxin">Digoxin</a>
 (796001), <a href="/web/20180712210714/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0012798&amp;fLockTerm=Diuretic">Diuretic</a>
 (30492008), <a href="/web/20180712210714/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0012798&amp;fLockTerm=Diuretic">Diuretic</a>
 (372695000), <a href="/web/20180712210714/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0018801&amp;fLockTerm=Heart+failure">Heart failure</a>
 (84114007), <a href="/web/20180712210714/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0020223&amp;fLockTerm=Hydralazine">Hydralazine</a>
 (22696000), <a href="/web/20180712210714/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0020223&amp;fLockTerm=Hydralazine">Hydralazine</a>
 (387125005), <a href="/web/20180712210714/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0162589&amp;fLockTerm=Implantable+defibrillator">Implantable defibrillator</a>
 (72506001), <a href="/web/20180712210714/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0022252&amp;fLockTerm=Isosorbide+dinitrate">Isosorbide dinitrate</a>
 (387332007), <a href="/web/20180712210714/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0022252&amp;fLockTerm=Isosorbide+dinitrate">Isosorbide dinitrate</a>
 (5924003), <a href="/web/20180712210714/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C1628973&amp;fLockTerm=Isosorbide+dinitrate+%2b+hydralazine">Isosorbide dinitrate + hydralazine</a>
 (419875005), <a href="/web/20180712210714/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0023212&amp;fLockTerm=Left+heart+failure">Left heart failure</a>
 (85232009), <a href="/web/20180712210714/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C1277187&amp;fLockTerm=Left+ventricular+systolic+dysfunction">Left ventricular systolic dysfunction</a>
 (134401001), <a href="/web/20180712210714/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0054015&amp;fLockTerm=Nesiritide">Nesiritide</a>
 (385678006), <a href="/web/20180712210714/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0054015&amp;fLockTerm=Nesiritide">Nesiritide</a>
 (385679003), <a href="/web/20180712210714/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0054015&amp;fLockTerm=Nesiritide">Nesiritide</a>
 (385680000), <a href="/web/20180712210714/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0054015&amp;fLockTerm=Nesiritide">Nesiritide</a>
 (407059007)                    </div>
                    <!-- /.field-content -->
                </div>
                <!-- /.field -->
