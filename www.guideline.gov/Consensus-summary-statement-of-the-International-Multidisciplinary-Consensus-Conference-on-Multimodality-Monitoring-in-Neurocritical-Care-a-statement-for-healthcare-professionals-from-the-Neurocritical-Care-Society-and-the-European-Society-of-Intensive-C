, <a href="/web/20180713033426/https://www.guideline.gov/search?f_MTH_CUI=C0011750&amp;fLockTerm=Developing+Countries">Developing Countries</a>
, <a href="/web/20180713033426/https://www.guideline.gov/search?f_MTH_CUI=C0013819&amp;fLockTerm=Electroencephalography">Electroencephalography</a>
, <a href="/web/20180713033426/https://www.guideline.gov/search?f_MTH_CUI=C0031691&amp;fLockTerm=ENOLASE">ENOLASE</a>
, <a href="/web/20180713033426/https://www.guideline.gov/search?f_MTH_CUI=C0019010&amp;fLockTerm=Hemodynamics">Hemodynamics</a>
, <a href="/web/20180713033426/https://www.guideline.gov/search?f_MTH_CUI=C0021368&amp;fLockTerm=Inflammation">Inflammation</a>
, <a href="/web/20180713033426/https://www.guideline.gov/search?f_MTH_CUI=C0085559&amp;fLockTerm=intensive+care">intensive care</a>
, <a href="/web/20180713033426/https://www.guideline.gov/search?f_MTH_CUI=C0021708&amp;fLockTerm=intensive+care+unit">intensive care unit</a>
, <a href="/web/20180713033426/https://www.guideline.gov/search?f_MTH_CUI=C0025088&amp;fLockTerm=Medical+Informatics">Medical Informatics</a>
, <a href="/web/20180713033426/https://www.guideline.gov/search?f_MTH_CUI=C0027765&amp;fLockTerm=nervous+system+disorder">nervous system disorder</a>
, <a href="/web/20180713033426/https://www.guideline.gov/search?f_MTH_CUI=C0027853&amp;fLockTerm=Neurologic+Examination">Neurologic Examination</a>
, <a href="/web/20180713033426/https://www.guideline.gov/search?f_MTH_CUI=C0028708&amp;fLockTerm=Nutrition+Assessment">Nutrition Assessment</a>
, <a href="/web/20180713033426/https://www.guideline.gov/search?f_MTH_CUI=C0523807&amp;fLockTerm=Oxygen+saturation+measurement">Oxygen saturation measurement</a>
, <a href="/web/20180713033426/https://www.guideline.gov/search?f_MTH_CUI=C0344106&amp;fLockTerm=Sedation+procedure">Sedation procedure</a>
, <a href="/web/20180713033426/https://www.guideline.gov/search?f_MTH_CUI=C0206077&amp;fLockTerm=Ultrasonography%2c+Doppler%2c+Transcranial">Ultrasonography, Doppler, Transcranial</a>
                    </div>
                    <!-- /.field-content -->
                </div>
                <!-- /.field -->
