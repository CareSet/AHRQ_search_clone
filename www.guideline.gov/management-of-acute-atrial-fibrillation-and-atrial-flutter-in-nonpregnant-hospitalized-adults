 (372821002), <a href="/web/20180712165330/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0002598&amp;fLockTerm=Amiodarone">Amiodarone</a>
 (69236009), <a href="/web/20180712165330/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0003280&amp;fLockTerm=Anticoagulant">Anticoagulant</a>
 (372862008), <a href="/web/20180712165330/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0003280&amp;fLockTerm=Anticoagulant">Anticoagulant</a>
 (81839001), <a href="/web/20180712165330/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0004238&amp;fLockTerm=Atrial+fibrillation">Atrial fibrillation</a>
 (49436004), <a href="/web/20180712165330/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0004239&amp;fLockTerm=Atrial+flutter">Atrial flutter</a>
 (5370000), <a href="/web/20180712165330/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0001645&amp;fLockTerm=beta-Blocking+agent">beta-Blocking agent</a>
 (33252009), <a href="/web/20180712165330/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0001645&amp;fLockTerm=beta-Blocking+agent">beta-Blocking agent</a>
 (373254001), <a href="/web/20180712165330/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0006684&amp;fLockTerm=Calcium+channel+blocker">Calcium channel blocker</a>
 (373304005), <a href="/web/20180712165330/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0006684&amp;fLockTerm=Calcium+channel+blocker">Calcium channel blocker</a>
 (48698004), <a href="/web/20180712165330/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0430467&amp;fLockTerm=Cardiac+electrophysiology">Cardiac electrophysiology</a>
 (252425004), <a href="/web/20180712165330/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0007203&amp;fLockTerm=Cardiopulmonary+resuscitation">Cardiopulmonary resuscitation</a>
 (89666000), <a href="/web/20180712165330/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0013778&amp;fLockTerm=Cardioversion">Cardioversion</a>
 (250980009), <a href="/web/20180712165330/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0012265&amp;fLockTerm=Digoxin">Digoxin</a>
 (387461009), <a href="/web/20180712165330/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0012265&amp;fLockTerm=Digoxin">Digoxin</a>
 (796001), <a href="/web/20180712165330/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0542380&amp;fLockTerm=Direct+current+cardioversion">Direct current cardioversion</a>
 (180325003), <a href="/web/20180712165330/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C1623258&amp;fLockTerm=Electrocardiographic+procedure">Electrocardiographic procedure</a>
 (29303009), <a href="/web/20180712165330/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0019993&amp;fLockTerm=Inpatient+care">Inpatient care</a>
 (394656005), <a href="/web/20180712165330/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0030695&amp;fLockTerm=Monitoring+of+patient">Monitoring of patient</a>
 (182777000), <a href="/web/20180712165330/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0031809&amp;fLockTerm=Physical+examination">Physical examination</a>
 (302199004), <a href="/web/20180712165330/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0031809&amp;fLockTerm=Physical+examination">Physical examination</a>
 (5880005), <a href="/web/20180712165330/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0031809&amp;fLockTerm=Physical+examination">Physical examination</a>
 (81375008), <a href="/web/20180712165330/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0039985&amp;fLockTerm=Plain+chest+X-ray">Plain chest X-ray</a>
 (399208008), <a href="/web/20180712165330/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0586087&amp;fLockTerm=Referral+to+neurologist">Referral to neurologist</a>
 (308474002)                    </div>
                    <!-- /.field-content -->
                </div>
                <!-- /.field -->
                <div class="field">
