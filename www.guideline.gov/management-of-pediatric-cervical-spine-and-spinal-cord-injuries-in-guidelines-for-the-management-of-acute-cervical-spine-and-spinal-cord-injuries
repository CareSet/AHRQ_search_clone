 (58825001), <a href="/web/20180712212108/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0040405&amp;fLockTerm=Computerized+axial+tomography">Computerized axial tomography</a>
 (77477000), <a href="/web/20180712212108/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0012691&amp;fLockTerm=Dislocation">Dislocation</a>
 (108367008), <a href="/web/20180712212108/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0012691&amp;fLockTerm=Dislocation">Dislocation</a>
 (87642003), <a href="/web/20180712212108/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0434543&amp;fLockTerm=Dislocation+of+atlanto-occipital+joint">Dislocation of atlanto-occipital joint</a>
 (263014005), <a href="/web/20180712212108/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0016356&amp;fLockTerm=Fluoroscopy">Fluoroscopy</a>
 (312275004), <a href="/web/20180712212108/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0016356&amp;fLockTerm=Fluoroscopy">Fluoroscopy</a>
 (44491008), <a href="/web/20180712212108/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0181278&amp;fLockTerm=Halo+brace">Halo brace</a>
 (183187001), <a href="/web/20180712212108/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0020944&amp;fLockTerm=Immobilization+-+action">Immobilization - action</a>
 (257884004), <a href="/web/20180712212108/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0432666&amp;fLockTerm=Injury+of+cervical+spine">Injury of cervical spine</a>
 (262522002), <a href="/web/20180712212108/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0027531&amp;fLockTerm=Injury+of+neck">Injury of neck</a>
 (90460009), <a href="/web/20180712212108/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0452049&amp;fLockTerm=Injury+of+nerves+and+spinal+cord+at+neck+level">Injury of nerves and spinal cord at neck level</a>
 (212355006), <a href="/web/20180712212108/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0024485&amp;fLockTerm=Magnetic+resonance+imaging">Magnetic resonance imaging</a>
 (113091000), <a href="/web/20180712212108/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0024485&amp;fLockTerm=Magnetic+resonance+imaging">Magnetic resonance imaging</a>
 (312250003), <a href="/web/20180712212108/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0336660&amp;fLockTerm=Magnetic+resonance+imaging+unit">Magnetic resonance imaging unit</a>
 (90003000), <a href="/web/20180712212108/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0079635&amp;fLockTerm=Orthopedic+internal+fixation+system">Orthopedic internal fixation system</a>
 (31031000), <a href="/web/20180712212108/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0030755&amp;fLockTerm=Pediatric+specialty">Pediatric specialty</a>
 (394537008), <a href="/web/20180712212108/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C1962945&amp;fLockTerm=Radiographic+imaging+procedure">Radiographic imaging procedure</a>
 (363680008), <a href="/web/20180712212108/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0203145&amp;fLockTerm=Radiography+of+cervical+spine">Radiography of cervical spine</a>
 (66769009), <a href="/web/20180712212108/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0203145&amp;fLockTerm=Radiography+of+cervical+spine">Radiography of cervical spine</a>
 (712970008), <a href="/web/20180712212108/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C1997068&amp;fLockTerm=Rotatory+subluxation+of+atlantoaxial+joint">Rotatory subluxation of atlantoaxial joint</a>
 (428799009), <a href="/web/20180712212108/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0037935&amp;fLockTerm=Spinal+arthrodesis">Spinal arthrodesis</a>
 (55705006), <a href="/web/20180712212108/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0037929&amp;fLockTerm=Spinal+cord+injury">Spinal cord injury</a>
 (90584004), <a href="/web/20180712212108/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0859273&amp;fLockTerm=Spinal+cord+injury+without+spinal+bone+injury">Spinal cord injury without spinal bone injury</a>
 (26738009), <a href="/web/20180712212108/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0037937&amp;fLockTerm=Spinal+injury">Spinal injury</a>
 (262521009), <a href="/web/20180712212108/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0040597&amp;fLockTerm=Traction">Traction</a>
