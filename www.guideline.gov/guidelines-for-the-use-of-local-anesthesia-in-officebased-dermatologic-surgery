 (95388000), <a href="/web/20180712203515/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0023660&amp;fLockTerm=Lidocaine">Lidocaine</a>
 (387480006), <a href="/web/20180712203515/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0023660&amp;fLockTerm=Lidocaine">Lidocaine</a>
 (82573000), <a href="/web/20180712203515/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0002921&amp;fLockTerm=Local+anesthesia">Local anesthesia</a>
 (386761002), <a href="/web/20180712203515/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0336644&amp;fLockTerm=Local+anesthesia+kit">Local anesthesia kit</a>
 (75187009), <a href="/web/20180712203515/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0198813&amp;fLockTerm=Local+anesthesia%2c+by+infiltration">Local anesthesia, by infiltration</a>
 (44596000), <a href="/web/20180712203515/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0002934&amp;fLockTerm=Local+anesthetic">Local anesthetic</a>
 (27548001), <a href="/web/20180712203515/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0002934&amp;fLockTerm=Local+anesthetic">Local anesthetic</a>
 (373477003), <a href="/web/20180712203515/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0027741&amp;fLockTerm=Nerve+block">Nerve block</a>
 (56333001), <a href="/web/20180712203515/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0027741&amp;fLockTerm=Nerve+block">Nerve block</a>
 (64874008), <a href="/web/20180712203515/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0002423&amp;fLockTerm=Outpatient+service">Outpatient service</a>
 (7271000175108), <a href="/web/20180712203515/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0033124&amp;fLockTerm=Prilocaine">Prilocaine</a>
 (19232009), <a href="/web/20180712203515/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0033124&amp;fLockTerm=Prilocaine">Prilocaine</a>
 (387107003), <a href="/web/20180712203515/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0002911&amp;fLockTerm=Regional+anesthesia">Regional anesthesia</a>
 (27372005), <a href="/web/20180712203515/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0040464&amp;fLockTerm=Topical+anesthetic">Topical anesthetic</a>
 (373520001), <a href="/web/20180712203515/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0040464&amp;fLockTerm=Topical+anesthetic">Topical anesthetic</a>
 (67585005)                    </div>
