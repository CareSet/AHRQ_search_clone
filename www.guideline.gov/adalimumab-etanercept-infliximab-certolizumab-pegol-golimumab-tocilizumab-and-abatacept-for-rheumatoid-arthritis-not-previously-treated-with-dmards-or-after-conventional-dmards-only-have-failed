 (421777009), <a href="/web/20180712202241/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0003209&amp;fLockTerm=Anti-inflammatory+preparations">Anti-inflammatory preparations</a>
 (330901000), <a href="/web/20180712202241/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0003191&amp;fLockTerm=Anti-rheumatic+agent">Anti-rheumatic agent</a>
 (88279005), <a href="/web/20180712202241/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C1872109&amp;fLockTerm=Certolizumab+pegol">Certolizumab pegol</a>
 (430306004), <a href="/web/20180712202241/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C1872109&amp;fLockTerm=Certolizumab+pegol">Certolizumab pegol</a>
 (430307008), <a href="/web/20180712202241/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C2353893&amp;fLockTerm=Golimumab">Golimumab</a>
 (442318002), <a href="/web/20180712202241/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C2353893&amp;fLockTerm=Golimumab">Golimumab</a>
 (442435002), <a href="/web/20180712202241/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0020852&amp;fLockTerm=Immunoglobulin+G">Immunoglobulin G</a>
 (29246005), <a href="/web/20180712202241/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0021031&amp;fLockTerm=Immunoglobulin%2c+F(ab)+fragment">Immunoglobulin, F(ab) fragment</a>
 (112136000), <a href="/web/20180712202241/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0021054&amp;fLockTerm=Immunologic+substance">Immunologic substance</a>
 (106181007), <a href="/web/20180712202241/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0021081&amp;fLockTerm=Immunosuppressant">Immunosuppressant</a>
 (372823004), <a href="/web/20180712202241/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0021081&amp;fLockTerm=Immunosuppressant">Immunosuppressant</a>
 (69431002), <a href="/web/20180712202241/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0666743&amp;fLockTerm=Infliximab">Infliximab</a>
 (108675009), <a href="/web/20180712202241/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0666743&amp;fLockTerm=Infliximab">Infliximab</a>
 (386891004), <a href="/web/20180712202241/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0003250&amp;fLockTerm=Monoclonal+antibody">Monoclonal antibody</a>
 (49616005), <a href="/web/20180712202241/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0003211&amp;fLockTerm=Non-steroidal+anti-inflammatory+agent">Non-steroidal anti-inflammatory agent</a>
 (16403005), <a href="/web/20180712202241/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0003211&amp;fLockTerm=Non-steroidal+anti-inflammatory+agent">Non-steroidal anti-inflammatory agent</a>
 (372665008), <a href="/web/20180712202241/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0032483&amp;fLockTerm=Polyethylene+glycol">Polyethylene glycol</a>
 (356076006), <a href="/web/20180712202241/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0032483&amp;fLockTerm=Polyethylene+glycol">Polyethylene glycol</a>
 (8030004), <a href="/web/20180712202241/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C0003873&amp;fLockTerm=Rheumatoid+arthritis">Rheumatoid arthritis</a>
 (69896004), <a href="/web/20180712202241/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C1609165&amp;fLockTerm=Tocilizumab">Tocilizumab</a>
 (444648007), <a href="/web/20180712202241/https://www.guideline.gov/search?f_SNOMEDCT_US_CUI=C1609165&amp;fLockTerm=Tocilizumab">Tocilizumab</a>
 (444649004)                    </div>
                    <!-- /.field-content -->
                </div>
                <!-- /.field -->
    </div>
